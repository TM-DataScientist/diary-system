{
  "name": "diary-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint \"**/*.{js,cjs,mjs}\" --no-error-on-unmatched-pattern",
    "lint:fix": "npm run lint -- --fix",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "prepare": "husky || true",
    "ci": "npm run lint && npm run test"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@prisma/client": "^7.4.1",
    "eslint": "^9.21.0",
    "husky": "^9.1.7",
    "prisma": "^7.4.1",
    "vitest": "^3.0.5"
  }
}

{
  "name": "diary-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"**/*.{js,cjs,mjs,ts,tsx}\" --no-error-on-unmatched-pattern",
    "lint:fix": "npm run lint -- --fix",
